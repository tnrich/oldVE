<?xml version="1.0" encoding="utf-8"?>
<!-- Author: Doug Densmore -->
<mx:Canvas 
	xmlns:mx="http://www.adobe.com/2006/mxml" 
	width="100%" 
	height="100%"
	>
    
    <mx:Box width="100%" height="100%" paddingBottom="10" paddingLeft="10" paddingRight="10" paddingTop="10">
        <mx:TileList id="imageTileList" width="100%" height="100%" borderStyle="none" paddingBottom="1" paddingLeft="1" paddingRight="1" paddingTop="1">
            <mx:itemRenderer>
                <mx:Component>
                    <mx:Box>
                        <mx:Box borderColor="0x999999" borderStyle="solid" borderThickness="1" paddingBottom="0" paddingLeft="0" paddingRight="0" paddingTop="0">
                            <mx:Image source="{data.forwardPath}" height="56" width="56" toolTip="{data.name}"/>
                        </mx:Box>
                    </mx:Box>
                </mx:Component>
            </mx:itemRenderer>
        </mx:TileList>
    </mx:Box>
	
</mx:Canvas>
